<?xml version='1.0' encoding='utf-8'?>
<widget id="com.study.scroller" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CDVWebViewEngine">
        <param name="ios-package" value="CDVWebViewEngine" />
    </feature>
    <feature name="LaunchScreen">
        <param name="ios-package" value="CDVLaunchScreen" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <name>Scroller</name>
    <description>The TikTok for learning</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <icon height="29" src="res/icons/ios/29.png" width="29" />
    <icon height="40" src="res/icons/ios/40.png" width="40" />
    <icon height="57" src="res/icons/ios/57.png" width="57" />
    <icon height="58" src="res/icons/ios/58.png" width="58" />
    <icon height="60" src="res/icons/ios/60.png" width="60" />
    <icon height="80" src="res/icons/ios/80.png" width="80" />
    <icon height="87" src="res/icons/ios/87.png" width="87" />
    <icon height="114" src="res/icons/ios/114.png" width="114" />
    <icon height="120" src="res/icons/ios/120.png" width="120" />
    <icon height="180" src="res/icons/ios/180.png" width="180" />
    <icon height="1024" src="res/icons/ios/1024.png" width="1024" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="MediaTypesRequiringUserActionForPlayback" value="none" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
</widget>
