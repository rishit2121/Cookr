<?xml version='1.0' encoding='utf-8'?>
<widget id="com.study.scroller" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Scroller</name>
    <description>The TikTok for learning</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <icon density="mdpi" src="res/icons/android/icon-mdpi.png" />
    <icon density="hdpi" src="res/icons/android/icon-hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/icon-xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/android/icon-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/android/icon-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="700000" />
</widget>
